<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["MOYO"] - MOYO by Safiya Nanji</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/style.css" />
 
</head>
<body>
    <header>
        <nav class="zone brown sticky">
            <ul class="main-nav">
                <li class="nav-item">
                    <a class="nav-link text-md-left" asp-area="" asp-page="/WelcomePage">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-md-left" asp-area="" asp-page="/AboutMe">About Me</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-page="/BookAppointment">Book Appointment</a>
                </li>
                <li class="push">
                    <a class="nav-link" asp-area="" asp-page="/MembershipRegistration">Sign Up</a>
                </li>
                <li class="push">
                    <a class="nav-link" asp-area="" asp-page="/ViewSchedule">Calendar</a>
                </li>
                <li class="push">
                    <a class="nav-link" asp-area="" asp-page="/Contact">Contact Me</a>
                </li>
            </ul>


        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @*<footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - MOYO_Website - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>*@

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
